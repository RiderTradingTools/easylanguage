					"name": "esl.class",
					"match": "\\b\\w+\\.(?![0-9]+\\b)\\w+\\b"
				}
			]
		},

		"var": {
			"patterns": [
				{
					"name": "variable.esl",
					"match": "(?i)(\\bvalue[0-9]+\\b|\\bplot[0-9]+\\b|\\bplot\\(|\\bplot\\s|\\bcondition[0-9]+\\b|\\bdata[0-9]+\\b|\\bdata\\b|NoPlot)"
				},
				{
					"name": "variable.type",
					"match": "(?i)\\b(INT|INT64|DOUBLE|FLOAT|String|IntrabarPersist|Null|BYTE|BOOL|CHAR|TRUE|FALSE|TrueFalse|TrueFalseArray|TrueFalseArrayRef|TrueFalseRef|TrueFalseSeries|TrueFalseSimple|WORD|DWORD|POINTER|UNSIGNED|LONG|LPBOOL|LPBYTE|LPDOUBLE|LPDWORD|LPFLOAT|LPINT|LPLONG|LPSTR|LPWORD|VARSIZE|VARSTARTADDR|Void|Array|Numeric|NumericSeries|NumericSimple|StringArray|StringArrayRef|StringRef|StringSeries|StringSimple)(?!\\.\\w)\\b"
				}
			]
		},

		"strings": {
			"patterns": [
				{
					"name": "string.quoted.single",
					"begin": "'",
					"end": "'"
				},
				{
					"name": "string.quoted.double",
					"begin": "\"",
					"end": "\""
				}
			]
		},

		"numbers": {
			"patterns": [
				{
					"name": "constant.numeric",
   				"match": "\\b[0-9]+\\.?[0-9]*\\b"
				},
				{
					"name": "constant.numeric",
   				"match": "\\.[0-9]+\\.?[0-9]*\\b"
				}
			]
		},

		"constants": {
			"patterns": [
				{
				"name": "constant.operator",
				"match": "=|\\*|/|<|<=|<>|!|\\+|\\+\\+|-|--|\\+=|-=|>=|>"
				}
			]
		},

		"regions": {
			"patterns": [
				{
					"name": "esl.control.section",
					"begin": "(?i)#\\b(BeginAlert|BeginCmtry|BeginCmtryOrAlert|End|EndRegion|Events|Region)\\b",
					"beginCaptures": {
						"0": {
							"name": "punctuation.definition.section"
						}
					},
					"end": "\n"
				}
			]
		},

		"comments": {
			"patterns": [
				{
					"name": "comment.block",
					"begin": "{",
					"beginCaptures": {
						"0": {
							"name": "punctuation.definition.comment.begin"
						}
					},
					"end": "}",
					"endCaptures": {
						"0": {
							"name": "punctuation.definition.comment.end"
						}
					}
				},
				{
					"name": "comment.line.double-slash",
					"begin": "//",
					"beginCaptures": {
						"0": {
							"name": "punctuation.definition.comment"
						}
					},
					"end": "\n"
				}
			]
		},

      "paren-expression": {
         "begin": "\\(",
         "end": "\\)",
         "beginCaptures": {
            "0": { "name": "punctuation.paren.open" }
         },
         "endCaptures": {
            "0": { "name": "punctuation.paren.close" }
         },
         "name": "expression.group",
         "patterns": [{ "include": "#expression" }]
      }

	}

}