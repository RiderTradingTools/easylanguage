{
    "For loop": {
      "prefix": ["for "],
      "body": ["for ${1:x} = ${2:1} to ${3:9} begin", "\t$0", "end;"]
    },
    "If (single line)": {
      "prefix": ["if "],
      "body": ["if $1 then$0"]
    },
    "If (code block)": {
      "prefix": ["if "],
      "body": ["if $1 then"
        "begin", 
        "\t$0", 
        "end;"]
    },
    "MarketPosition": {
      "prefix": ["if MarketPosition"],
      "body": ["if MarketPosition ${1|=,>,<,<>|} ${2|0,-1,1|} then begin", "\t$0", "end;"]
    },
    "Buy Order": {
      "prefix": ["BuyToCover"],
      "body": ["${1|Buy,BuyToCover|} next bar ${2:market};"]
    },
    "Sell Order": {
      "prefix": ["SellShort"],
      "body": ["${1|Sell,SellShort|} next bar ${2:market};"]
    },
    "Description for signal": {
      "prefix": ["setting"],
      "body": [
        "$BLOCK_COMMENT_START",
        "\tInstruments: ${1:TXF}",
        "\tResolution: ${2:3}K",
        "\tSession: ${3:0845-1330}",
        "\t$0",
        "$BLOCK_COMMENT_END"
      ]
    },
    "3 Long candles": {
      "prefix": ["3long", "GGG"],
      "body": ["o>c ${1:and o[1]>c[1]} ${2:and o[2]>c[2]}"]
    },
    "3 Short candles": {
      "prefix": ["3short", "RRR"],
      "body": ["c>o ${1:and c[1]>o[1]} ${2:and c[2]>o[2]}"]
    },
    "Print": {
      "prefix": ["print"],
      "body": ["print($0);"]
    },
    "Reducing positions": {
      "prefix": ["if currentcontracts", "currentcontracts"],
      "body": ["if currentcontracts > 1 then begin", "\t$0", "end;"]
    },
    "Cross": {
      "prefix": ["cross"],
      "body": ["cross ${1|over,below|}"]
    },
    "EntriesToday": {
      "prefix": ["EntriesToday"],
      "body": ["EntriesToday(D) < ${1:1}"]
    },
    "BarsSinceEntry": {
      "prefix": ["BarsSinceEntry"],
      "body": ["BarsSinceEntry(${1:0})"]
    },
    "Highest": {
      "prefix": ["highest"],
      "body": ["highest(${1:close}, ${2:5})"]
    },
    "Lowest": {
      "prefix": ["lowest"],
      "body": ["lowest(${1:close}, ${2:5})"]
    },
    "Begin": {
      "prefix": ["begin"],
      "body": ["begin", "\t$0", "end;"]
    },
    "Daily low price":{
      "prefix": ["LowD"],
      "body": ["LowD(${1:0})"]
    },
    "Daily high price":{
      "prefix": ["HighD"],
      "body": ["HighD(${1:0})"]
    },
    "Market":{
      "prefix": ["Market"],
      "body": ["Market;"]
    },
    "While loop": {
      "prefix": ["while"],
      "body": ["while ${1:condition} begin", "\t$0", "end;"]
    },

}