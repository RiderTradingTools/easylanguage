	"tokenColors": [


		// easylanguage syntax colors ---------------------------------

		{	// strings
			"scope": [
				"string",
				"string.quoted.single",
				"string.quoted.double"
			],
			"settings": {
				"foreground": "#2cdc05"
			}
		},
      {	// comments
			"scope": [
                "comment",
                "comment.block"
            ],
			"settings": {
				"foreground": "#65492ed6",
                "fontStyle": "italic"
			}
		},
		{	// reserved keywords
			"scope": "esl.reserved",
			"settings": {
				"foreground": "#ddff33db"
			}
		},
		{	// functions
			"scope": "esl.function",
			"settings": {
				"foreground": "#e05D38"
			}
		},
		{	// classes
			"scope": "esl.class",
			"settings": {
				"foreground": "#00e0b0"
			}
		},
		{	// control words: begin, end, if, then, etc
			"scope": "esl.control.keyword",
			"settings": {
				"foreground": "#569CD6"
			}
		},
		{	// sectional control words: region, etc
			"scope": "esl.control.section",
			"settings": {
				"foreground": "#01d6d675",
				"fontStyle": "italic bold",
			}
		},
		{	// price data:  High/Low/Open/Close
			"scope": "esl.pricedata",
			"settings": {
				"foreground": "#0379ac",
                "fontStyle": "bold"
			}
		},
		{	// numbers
			"scope": [
				"constant.numeric",
				"variable.other.enummember",
				"keyword.operator.plus.exponent",
				"keyword.operator.minus.exponent"
			],
			"settings": {
				"foreground": "#ca41bd",
                "fontStyle": "bold"
			}
		},
		{	// operators
			"scope": "constant.operator",
			"settings": {
				"foreground": "#7f8e81"
			}
		},
		{	// skip words
			"scope": "esl.skipwords",
			"settings": {
				"foreground": "#ff9e9e",
                "fontStyle":"italic"
			}
		},
		{	// var scope: int, double, bool, True/False, etc
			"scope": "variable.type",
			"settings": {
				"foreground": "#b87b02"
			}
		},
		{	// Value1, Plot1, Condition1, etc
			"scope": "variable.esl",
			"settings": {
				"foreground": "#D7BA7D"
			}
		},






















		{
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#ccb697"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#b99768"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#ccb697",
				"fontStyle": ""
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#8b6e46",
				"fontStyle": ""
			}
		},
		{
			"scope": "entity.name.class",
			"settings": {
				"foreground": "#ccb697",
				"fontStyle": "underline"
			}
		},
		{
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#ccb697",
				"fontStyle": ""
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#e2cdb0",
				"fontStyle": ""
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#ffffff",
				"fontStyle": ""
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#8b6e46",
				"fontStyle": ""
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#ccb697",
				"fontStyle": ""
			}
		},
		{
			"scope": "support.constant",
			"settings": {
				"foreground": "#ccb697",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": "#ccb697",
				"fontStyle": ""
			}
		},
		{
			"scope": "support.other.variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#cf433e",
				"background": "#664e4d",
				"fontStyle": ""
			}
		},
		{
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#cf433e",
				"background": "#664E4D"
			}
		},
		{
			"scope": "meta.structure.dictionary.json string.quoted.double.json",
			"settings": {
				"foreground": "#f7a21b"
			}
		},
		{
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"foreground": "#795431"
			}
		},
		{
			"scope": "constant.numeric.line-number.find-in-files - match",
			"settings": {
				"foreground": "#8FBE00A0"
			}
		},
		{
			"scope": "entity.name.filename.find-in-files",
			"settings": {
				"foreground": "#E6DB74"
			}
		},
		{
			"scope": "keyword.other",
			"settings": {
				"foreground": "#af7a47"
			}
		},
		{
			"scope": [
				"meta.property-value",
				"support.constant.property-value",
				"constant.other.color"
			],
			"settings": {
				"foreground": "#f7a21b"
			}
		},
		{
			"scope": "meta.structure.dictionary.json string.quoted.double.json",
			"settings": {
				"foreground": "#ccb697"
			}
		},
		{
			"scope": "meta.structure.dictionary.value.json string.quoted.double.json",
			"settings": {
				"foreground": "#f7a21b"
			}
		},
		{
			"scope": "meta.property-name support.type.property-name",
			"settings": {
				"fontStyle": "normal"
			}
		},
		{
			"scope": "meta.property-value punctuation.separator.key-value",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": [
				"keyword.other.use",
				"keyword.other.function.use",
				"keyword.other.namespace",
				"keyword.other.new",
				"keyword.other.special-method",
				"keyword.other.unit",
				"keyword.other.use-as"
			],
			"settings": {
				"foreground": "#8b6e46"
			}
		},
		{
			"scope": [
				"meta.use support.class.builtin",
				"meta.other.inherited-class support.class.builtin"
			],
			"settings": {
				"foreground": "#8b6e46",
				"fontStyle": "normal"
			}
		},
		{
			"scope": "variable.other",
			"settings": {
				"foreground": "#ffffff",
				"fontStyle": "normal"
			}
		},
		{
			"scope": "markup.deleted.git_gutter",
			"settings": {
				"foreground": "#e61f44"
			}
		},
		{
			"scope": "markup.inserted.git_gutter",
			"settings": {
				"foreground": "#a7da1e"
			}
		},
		{
			"scope": "markup.changed.git_gutter",
			"settings": {
				"foreground": "#f7b83d"
			}
		},
		{
			"scope": "meta.template.expression",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796E6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#CD9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#F44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
